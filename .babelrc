{
  "presets": [
      "minify",
      "@babel/preset-env",
      "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "components": "./components",
          "forms": "./forms",
          "pages": "./pages"
        }
      }
    ]
  ]
}
